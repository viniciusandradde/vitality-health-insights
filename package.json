{
  "name": "vsa-analytics-health",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "turbo dev",
    "dev:frontend": "turbo dev --filter=@vsa/frontend",
    "dev:backend": "turbo dev --filter=@vsa/backend",
    "dev:admin": "turbo dev --filter=@vsa/admin",
    "build": "turbo build",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "docker:up": "docker-compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.yml down",
    "docker:logs": "docker-compose -f docker/docker-compose.yml logs -f",
    "docker:dev:start": "./scripts/dev-docker.sh start",
    "docker:dev:stop": "./scripts/dev-docker.sh stop",
    "docker:dev:logs": "./scripts/dev-docker.sh logs",
    "docker:dev:ps": "./scripts/dev-docker.sh ps",
    "docker:dev:build": "./scripts/dev-docker.sh build"
  },
  "devDependencies": {
    "pnpm": "^10.26.0",
    "turbo": "^2.7.4"
  }
}
